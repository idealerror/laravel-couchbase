{
    "name": "idealerror/laravel-couchbase",
    "description": "A Couchbase based Eloquent model and Query builder for Laravel",
    "keywords": ["laravel","eloquent","couchbase","database","model"],
    "homepage": "https://github.com/hafsalmetvr/laravel-couchbase",
    "authors": [
    ],
    "license" : "MIT",
    "require": {
        "ext-couchbase": ">=3.0.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-0": {
            "Mpociot\\Couchbase": "src/",
            "Mpociot\\Eloquent": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models",
            "tests/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpociot\\Couchbase\\CouchbaseServiceProvider"
            ]
        }
    }
}
